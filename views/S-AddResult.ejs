<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Add Result</title>
</head>

<body>
    <form method="post">
        <table>
            <tr>
                <td>Enter Name</td>
                <td><input type="text" name="sname"></td>
            </tr>
            <tr>
                <td>Enter Standard</td>
                <td>
                    <select name="stnd">
                        <% result1.forEach(function(data) { %>
                            <option value="<%= data.stnd %>"><%= data.stnd %></option>
                        <% }); %>
                    </select>
                </td>
            </tr>
            <tr>
                <td>Enter Division</td>
                <td>
                    <select name="sdiv">
                        <% result1.forEach(function(data) { %>
                            <option value="<%= data.sdiv %>"><%= data.sdiv %></option>
                        <% }); %>
                    </select>
                </td>
            </tr>
            <tr>
                <td>Enter Roll</td>
                <td>
                    <select name="sroll">
                        <% result2.forEach(function(data) { %>
                            <option value="<%= data.sroll %>"><%= data.sroll %></option>
                        <% }); %>
                    </select>
                </td>
            </tr>
            <tr>
                <td>Enter Sub1 Marks</td>
                <td><input type="text" name="sub1"></td>
            </tr>
            <tr>
                <td>Enter Sub2 Marks</td>
                <td><input type="text" name="sub2"></td>
            </tr>
            <tr>
                <td>Enter Sub3 Marks</td>
                <td><input type="text" name="sub3"></td>
            </tr>
            <tr>
                <td>Enter Sub4 Marks</td>
                <td><input type="text" name="sub4"></td>
            </tr>
            <tr>
                <td>Enter Sub5 Marks</td>
                <td><input type="text" name="sub5"></td>
            </tr>
            <tr>
                <td><input type="submit" value="Submit"></td>
            </tr>
        </table>
    </form>
</body>

</html>