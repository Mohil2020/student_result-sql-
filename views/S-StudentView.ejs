<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Student View</title>
</head>
<body>
    <table border="" style="text-align: center">
        <tr>
            <th>Student Name</th>
            <th>Student Standard</th>
            <th>Student Division</th>
            <th>Student Roll</th>
        </tr>
        <% result1.forEach(function(data){%>
        <tr>
            <td><%=data.name%></td>
            <td><%=data.stnd%></td>
            <td><%=data.sdiv%></td>
            <td><%=data.sroll%></td>
        </tr>
        <%})%>
    </table>
</body>
</html>